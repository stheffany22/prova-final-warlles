
#include <stdio.h>

int main() {
    float total = 780000.0;     // valor total do prêmio
    float primeiro, segundo, terceiro;

    // cálculos dos percentuais
    primeiro = total * 0.46;    // 46% do total
    segundo = total * 0.32;     // 32% do total
    terceiro = total - (primeiro + segundo); // resto vai para o terceiro

    // saída formatada
    printf("Primeiro ganhador: R$ %.2f\n", primeiro);
    printf("Segundo ganhador: R$ %.2f\n", segundo);
    printf("Terceiro ganhador: R$ %.2f\n", terceiro);

    return 0;
}
